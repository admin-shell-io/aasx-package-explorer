                        <AnyUiFlyoutMessageBox 
                            EventSession="evs" 
                            DialogueData="evs.DialogueData"/>
                    }

                    @if (evs.DialogueData is AnyUiDialogueDataTextBox)
                    {
                        <AnyUiFlyoutTextBox
                            EventSession="evs"
                            DialogueData="evs.DialogueData" />
                    }

                    @if (evs.DialogueData is AnyUiDialogueDataTextEditor)
                    {
                        <AnyUiFlyoutTextEditor 
                            EventSession="evs"
                            DialogueData="evs.DialogueData" />
                    }

