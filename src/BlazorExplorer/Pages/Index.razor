    protected void UpdateTreeWithTouchedValues()
    {
        // access
        var root = Session.ElementPanel;
        if (root == null)
            return;

        // recurse over all
        foreach (var el in root.FindAll())
            if (el?.Touched == true && el.DisplayData is AnyUiDisplayDataHtml dd)
            {
                // clear
                el.Touched = false;

                // trigger action
                dd.TouchLambda?.Invoke(el);
            }
    }

