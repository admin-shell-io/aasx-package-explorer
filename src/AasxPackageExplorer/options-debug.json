    {
      "Name": "Test 5",
      "Lines": [
        "Tool(\"EditMenu\", \"Mode\", true);",
        "res = Tool(\"ToolsFindText\", \"FindText\", \"DE\"); ",
        "num = 0;",
        "while (res) {",
        "    x = Select(\"This\"); ",
        "    WriteLine(num, \":\", x.idShort);",
        "    num = num + 1;",
        "    if (num > 18)",
        "        break;",
        "    res = Tool(\"ToolsFindForward\");",
        "    Sleep(200);",
        "}"
      ]
    },
